<h1>New Member</h1>
<%= form_for @member do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :gender %>
  <%= f.text_field :gender %>

  <%= f.label :eye_color %>
  <%= f.text_field :eye_color %>

  <%= f.label :hair_color %>
  <%= f.text_field :hair_color %>

  <%= f.submit %>
<% end %>
